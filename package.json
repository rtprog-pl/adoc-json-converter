{
  "name": "@rtprog/adoc-json-converter",
  "version": "0.0.1",
  "description": "AsciiDoctor.js JSON Converter",
  "main": "src/index.cjs",
  "module": "src/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.mjs",
      "require": "./src/index.cjs"
    },
    "mdast": {
      "import": "./src/mdast.mjs",
      "require": "./src/mdast.cjs"
    }
  },
  "jest": {
    "transform": {}
  },
  "scripts": {
    "build": "rollup src/index.mjs --file src/index.cjs --format cjs && rollup src/mdast.mjs --file src/mdast.cjs --format cjs",
    "test": "node test/test.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtprog-pl/adoc-json-converter.git"
  },
  "keywords": [
    "adoc",
    "asciidoc",
    "asciidoctor",
    "converter",
    "json",
    "mdast"
  ],
  "author": "Ryszard Trojnacki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rtprog-pl/adoc-json-converter/issues"
  },
  "homepage": "https://github.com/rtprog-pl/adoc-json-converter#readme",
  "devDependencies": {
    "@types/mdast": "^4.0.4",
    "@types/unist": "^3.0.3",
    "asciidoctor": "^3.0.4",
    "jest": "^29.7.0",
    "mdast-util-from-markdown": "^2.0.1",
    "rollup": "^4.21.2",
    "unist-util-remove-position": "^5.0.0"
  },
  "dependencies": {
    "@rtprog/json-string": "^0.0.2"
  }
}
